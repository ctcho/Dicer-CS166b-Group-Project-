<p id="notice"><%= notice %></p>

<h1>Dm Bios</h1>

<table>
  <thead>
    <tr>
      <th>Bio</th>
      <th>Exp level</th>
      <th>Ruleset1</th>
      <th>Ruleset2</th>
      <th>Ruleset3</th>
      <th>Ruleset4</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dm_bios.each do |dm_bio| %>
      <tr>
        <td><%= dm_bio.bio %></td>
        <td><%= dm_bio.exp_level %></td>
        <td><%= dm_bio.ruleset1 %></td>
        <td><%= dm_bio.ruleset2 %></td>
        <td><%= dm_bio.ruleset3 %></td>
        <td><%= dm_bio.ruleset4 %></td>
        <td><%= link_to 'Show', dm_bio %></td>
        <td><%= link_to 'Edit', edit_dm_bio_path(dm_bio) %></td>
        <td><%= link_to 'Destroy', dm_bio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dm Bio', new_dm_bio_path %>
